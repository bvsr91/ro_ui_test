<core:FragmentDefinition xmlns="sap.m"
    xmlns:f="sap.ui.layout.form"
    xmlns:semantic="sap.f.semantic"
    xmlns:footerbar="sap.ushell.ui.footerbar"
    xmlns:core="sap.ui.core">
    <Dialog id="myDialog" title="History Comments .." contentHeight="38%" contentWidth="62%">
        <content>
            <List  id="commentsList" showSeparators="Inner" items="">
                <FeedListItem sender="{approver}"  iconDensityAware="false" info="{Type}" timestamp="{Date}" text="{Text}" convertLinksToAnchorTags="All"/>
            </List>
        </content>
        <buttons>
            <Button text="Cancel" press="onCloseCommentsData" type="Emphasized"/>
        </buttons>
    </Dialog>
</core:FragmentDefinition>