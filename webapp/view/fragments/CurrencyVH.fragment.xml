<core:FragmentDefinition xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <SelectDialog noDataText="No Currencies Found" title="Select Currency" search=".onSearch" confirm=".onValueHelpDialogCurrencyClose" cancel=".onValueHelpDialogCurrencyClose" showClearButton="true" items="{
			path: '/Currencies',
			sorter: {
				path: 'code',
				descending: false
			}
		}">
        <StandardListItem title="{code}" description="{descr}" type="Active" />
    </SelectDialog>
</core:FragmentDefinition>