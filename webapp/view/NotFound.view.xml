<mvc:View controllerName="com.ferrero.zmrouiapp.controller.NotFound"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:f="sap.f"
    xmlns:l="sap.ui.layout">
    <MessagePage showHeader="false"
				 text="{i18n>notFound}"
				 enableFormattedText="true"
				 description="{i18n>notFound.text}"
				 icon="sap-icon://message-warning">
		<buttons>
			<Button text="Reload page" press="onReload"/>
		</buttons>
	</MessagePage>
</mvc:View>